doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body
        table 
            thead 
                tr 
                    th
                        h1 Id 
                    th 
                        h1 Name 
                    th 
                        h1 Visits
            tbody 
                each User in users 
                    tr 
                        td=visitor._id 
                        td=visitor.name 
                        td=visitor.count 